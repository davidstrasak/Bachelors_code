{
   "printWidth": 200,
   "tabWidth": 3,
   "useTabs": false,
   "semi": true,
   "singleQuote": false,
   "trailingComma": "none",
   "bracketSpacing": true,
   "arrowParens": "avoid",
   "proseWrap": "preserve",
   "endOfLine": "auto",
   "overrides": [
      {
         "files": ["*.h", "*.cpp"],
         "options": {
            "parser": "cpp"
         }
      }
   ]
}
